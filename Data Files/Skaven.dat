      </link>
    <link option="ratOgres" display="103" index="5">
      <exclref id="ratOgres"/>
      <live>Group.MenOWar</live>
      </link>
    <link option="ratOgres" display="103" rangemax="3" costtype="single" index="3">
      <exclref id="ratOgres"/>
      <live>Group.ShipOTLine</live>
      </link>
    <link option="slaves" display="100" rangemax="100" index="0">
