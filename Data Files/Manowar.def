  <profile id="fly1perK" name="Flyers" dynamic="yes" minvalue="0" maxvalue="0">
    <qualify>Group.Flyers</qualify>
    <minmax>var max as number
max = round(roster.usersize/1000,0,-1)
@maxvalue = max
@maxtext = max
@maxbar = max</minmax>
    </profile>
  <profile id="ally" name="Ally" dynamic="yes" minvalue="0" maxvalue="0" maxbar="100">
    <qualify>ally.?</qualify>
    <finaltest><![CDATA[var max as number
max = roster.usersize/3.0
if (this.cost <= max) then
 @valid = 1
endif
@bar = (this.cost/max) * 100
@text = @bar & "%"]]></finaltest>
    </profile>
